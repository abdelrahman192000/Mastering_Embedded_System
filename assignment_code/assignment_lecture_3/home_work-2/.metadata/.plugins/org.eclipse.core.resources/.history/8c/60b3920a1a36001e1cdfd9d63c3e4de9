#include <stdio.h>

int main (void)
{
	int num_1,num_2,num_3 ;
	printf("Enter three number:");
	fflush(stdout);
	scanf("%d %d %d",&num_1,&num_2,&num_3);

	if (num_1>num_2 &&num_1> num_3)
		printf("Largest number = %d",num_1);
	if (num_2>num_1 &&num_2> num_3)
		printf("Largest number = %d",num_2);
	else
		printf("Largest number = %d",num_1);



}
