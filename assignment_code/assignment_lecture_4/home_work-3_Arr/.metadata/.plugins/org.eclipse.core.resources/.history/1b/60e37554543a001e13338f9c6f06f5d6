#include <stdio.h>
int main (void)
{
	int number,i;
	float numbers_arr[30];
	printf("Enter the numbers of data: ");
	fflush(stdout);
	scanf("%d",&number);

	for (i=0;i<number;i++)
	{
		printf("Enter number: ");
		fflush(stdout);
		scanf("%f",&numbers_arr[i]);


	}

}



