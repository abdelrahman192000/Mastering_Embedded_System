#include <stdio.h>

int main (void)
{
	char arr[10],reverced_arr[10];
	int i,x,v;

	printf("Enter the string :");
	fflush(stdout);
	scanf("%s",arr);
	v=0;
	for(i=0;arr[i]!='\0';i++);
	for (x=i;x!=0;x--)
	{
		reverced_arr[x]=arr[v];
		v++
	}

	printf("\n%s",reverced_arr);

}


