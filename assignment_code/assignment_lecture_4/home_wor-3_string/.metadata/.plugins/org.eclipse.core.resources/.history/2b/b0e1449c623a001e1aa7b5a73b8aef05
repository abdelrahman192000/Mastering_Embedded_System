#include <stdio.h>

int main (void)
{
	char arr[10],reverced_arr[10];
	int i,x;

	printf("Enter the string :");
	fflush(stdout);
	scanf("%s",arr);

	for(i=0;arr[i]!='\0';i++);
	for (x=0;x<i;x++)
	{
		reverced_arr[x]=arr[x];
	}

	printf("\n%s",reverced_arr);

}


