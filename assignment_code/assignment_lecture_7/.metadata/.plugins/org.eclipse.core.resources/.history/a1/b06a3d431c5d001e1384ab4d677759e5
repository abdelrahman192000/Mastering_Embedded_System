#include <stdio.h>
#include <math.h>


int main (void)
{
	int count=0,i=1,div=1,first_n,second_n,q=100,p=55,j;
	int number=0;

	//for ( j=p;j<q;j++)
	//{
	j=p;
		number = j*j ;
		while (number!=0)
		{
			printf("\n   %d\n",number);
			number/=10;

			count++;
		}

		while(i<=(count/2))
		{

			div*=10;
			printf("%d\n",div);
			i++;
		}

		first_n=(number)%div;
		second_n=(number)/div;

		printf("first = %d\n",first_n);
		printf("second = %d\n",second_n);

		if((first_n+second_n)<=j)
		{
			printf("ok\n");

		}
	//}


}


