#include <stdio.h>



int main (void)
{
	int i,x;
	char  days [7][3] = { {"sun"}, {"mon"}, {"tus"},{"wen"},{"thr"},{"fri"},{"sat"}};


	for (i=0;i<8;i++)
	{
		for (x=0;x<3;x++)
		{
			printf("%c ",days [i][x]);

		}
		printf("\n");

	}






}


