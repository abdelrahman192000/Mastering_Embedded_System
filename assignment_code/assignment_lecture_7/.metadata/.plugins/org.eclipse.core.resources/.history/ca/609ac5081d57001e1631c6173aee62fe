#include <stdio.h>



int main (void)
{
	int i,x;
	char  days [6][3] = { {"sun"}, {"mon"},{"sun"}, {"tus"},{"wen"},{"thr"}};


	for (i=0;i<7;i++)
	{
		for (x=0;x<3;x++)
		{
			printf("%c",days[0][i]);

		}
		printf("%c",days[0][i]);

	}






}


